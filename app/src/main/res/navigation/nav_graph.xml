<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/searchCatListFragment">

    <fragment
        android:id="@+id/searchCatListFragment"
        android:name="com.example.cutecat.view.screens.SearchCatListFragment"
        android:label="fragment_search_cat_list"
        tools:layout="@layout/fragment_search_cat_list" >
        <action
            android:id="@+id/action_searchCatListFragment_to_fullImageFragment"
            app:destination="@id/fullImageFragment" />
    </fragment>
    <fragment
        android:id="@+id/fullImageFragment"
        android:name="com.example.cutecat.view.screens.FullImageFragment"
        android:label="fragment_full_image"
        tools:layout="@layout/fragment_full_image" >
        <argument android:name="CatArg" />
    </fragment>
</navigation>